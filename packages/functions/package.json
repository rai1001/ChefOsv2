{
  "name": "@culinaryos/functions",
  "version": "2.0.0",
  "type": "module",
  "description": "Firebase Cloud Functions",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "serve": "pnpm build && firebase emulators:start --only functions",
    "shell": "pnpm build && firebase functions:shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "@culinaryos/core": "workspace:*",
    "firebase-admin": "^13.0.1",
    "firebase-functions": "^6.1.3",
    "@google-cloud/documentai": "^8.0.0",
    "@google-cloud/vertexai": "^1.10.0",
    "@slack/web-api": "^7.13.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "octokit": "^5.0.5",
    "pdfkit": "^0.17.2",
    "resend": "^6.6.0",
    "stripe": "^20.1.0",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/pdfkit": "^0.17.4",
    "@types/cors": "^2.8.17",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": "20"
  }
}