{
  "buildCommand": "pnpm -r build",
  "outputDirectory": "packages/web/dist",
  "framework": "vite",
  "installCommand": "npm install -g pnpm@9.15.0 && pnpm install --frozen-lockfile",
  "devCommand": "cd packages/web && npm run dev",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
